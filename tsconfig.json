{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom",
      "webworker"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "~/*": ["src/*"]
    },
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "types"
    ]
  }
}
